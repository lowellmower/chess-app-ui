
<div class="well container">
  <h1>Static Pages</h1>
</div>
  <% if user_signed_in? %>
    <% @users.each do |user|  %>
    <h3> <%= user.name %></h3>
    <li> <%= link_to 'Request Game', new_invitation_path(sender_id: current_user.id, recipient_id: user.id), remote: true, class: 'btn btn-success' %></li>
  <% end %>
<% end %>


